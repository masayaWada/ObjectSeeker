"""
ObjectSeeker 設定ファイル例
"""

# ObjectSeeker 設定ファイル
# このファイルを ~/.objectseeker/config.json に配置することで、
# アプリケーションの動作をカスタマイズできます。

{
    "window": {
        "width": 1000,
        "height": 700,
        "resizable": true
    },
    "search": {
        "max_results": 100,
        "timeout": 30
    },
    "azure": {
        "graph_api_version": "v1.0",
        "resource": "https://graph.microsoft.com"
    },
    "ui": {
        "theme": "default",
        "font_size": 10
    },
    "logging": {
        "level": "INFO",
        "max_log_files": 7
    }
}